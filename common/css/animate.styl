@keyframes load {
	0%,
	100% {
		transform: scaleY(0.2);
	}
	16.67% {
		transform: scaleY(0.6);
	}
	33.33% {
		transform: scaleY(0.2);
	}
	50% {
		transform: scaleY(0.6);
	}
	66.67% {
		transform: scaleY(0.2);
	}
	83.34% {
		transform: scaleY(0.6);
	}
}

@keyframes download-wave
	0%
		filter hue-rotate(0deg)

	100%
		filter hue-rotate(361deg)

